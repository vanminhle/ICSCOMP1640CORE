@model ICSCOMP1640CORE.Models.Coordinator


        <link rel="stylesheet" href="~/css/InforCoordinator/Infor.css" />
    <!---<title> Responsive Registration Form | CodingLab </title>--->

<body>
	<div class="infor">
			<div class="title">
				<div class="title-Infor"style="font-size:40px;text-align: center;border-bottom:1px solid black; padding-bottom:20px;">
					Information Coordinator
				</div>
				<div class="containerInfor">
					<div class="element">
				FullName :&nbsp;&nbsp; @Model.User.FullName
				</div>
				<div class="element">
				Email:&nbsp;&nbsp;	@Model.User.Email
				</div >
				<div class="element">
				Address:&nbsp;&nbsp; @Model.User.Address
				</div>
				<div class="element">
					Age:&nbsp;&nbsp;@Model.User.Age
				</div>
				<div class="element">
				Department Name :&nbsp;&nbsp; @Model.Department.DepartmentName
			</div>
				</div>
				
			<a asp-controller="Admins" asp-action="Index" class="btn btn-dark">Back</a>
		
		</div>

			
	</div>
	</body>


